{
    "version": "0.1",
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "declaration": false,
        "noImplicitAny": false,
        "removeComments": true,
        "noLib": false,
        "preserveConstEnums": true,
        "suppressImplicitAnyIndexErrors": true
    },
    "filesGlob": [
        "./www/app/**/*.ts",
        "./typings/**/*.ts",
        "./local_typings/**/*.ts",
        "!./node_modules/**/*.ts",
		"!./www/lib/**/*.ts"
    ],
    "files": []
}